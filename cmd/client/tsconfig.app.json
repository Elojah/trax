{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "../../cmd/api/grpc/*"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@pkg/*": [
				"../../pkg/*"
			],
			"@internal/*": [
				"../../internal/*"
			],
			"@api/*": [
				"../../cmd/api/grpc/*"
			],
    },
  }
}
