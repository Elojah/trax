{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "../../pkg/**/*.ts",
    "../../internal/**/*.ts",
    "../../cmd/api/grpc/**/*.ts"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@pkg/*": [
        "../../pkg/*"
      ],
      "@internal/*": [
        "../../internal/*"
      ],
      "@api/*": [
        "../../cmd/api/grpc/*"
      ],
      "@protobuf-ts/*": [
        "./node_modules/@protobuf-ts"
      ],
    },
  }
}
